{
  "name": "Musl",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run tsc --",
    "clean": "rimraf artifacts",
    "lint": "tslint src/**/*.ts{,x}",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "test": "jest",
    "tsc": "tsc",
    "watch": "npm run build -- -w"
  },
  "dependencies": {
    "@react-native-community/push-notification-ios": "^1.0.2",
    "@stomp/stompjs": "^5.4.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "convert-units": "^2.3.4",
    "es6-symbol": "^3.1.3",
    "eventemitter2": "^5.0.1",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "moment": "^2.29.4",
    "native-base": "^2.13.8",
    "pluralize": "^8.0.0",
    "react": "16.11.0",
    "react-native": "^0.61.2",
    "react-native-admob": "^1.3.2",
    "react-native-ci-tools": "^1.1.0",
    "react-native-circular-slider": "^1.0.1",
    "react-native-cookie": "github:shimohq/react-native-cookie#b981acd",
    "react-native-datepicker": "^1.7.2",
    "react-native-exit-app": "^1.1.0",
    "react-native-fast-image": "^7.0.2",
    "react-native-firebase": "^5.5.6",
    "react-native-i18n": "^2.0.15",
    "react-native-image-crop-picker": "^0.26.1",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-maps": "^0.26.1",
    "react-native-material-textfield": "^0.16.1",
    "react-native-multi-slider": "github:NursultanAzamatZensoft/react-native-multi-slider",
    "react-native-photo-view": "^1.5.2",
    "react-native-push-notification": "^3.1.9",
    "react-native-square-grid": "^1.0.5",
    "react-native-svg": "^9.13.3",
    "react-native-swipeable": "^0.6.0",
    "react-native-tag-input": "github:NursultanAzamatZensoft/react-native-tag-input",
    "react-native-version-check": "^3.3.0",
    "react-native-video": "^5.0.2",
    "react-native-video-controls": "^2.2.3",
    "react-navigation": "^4.0.10",
    "react-string-replace": "^0.4.4",
    "realm": "^3.3.0",
    "reflect-metadata": "^0.1.13",
    "sockjs-client": "github:datso/sockjs-client",
    "typescript-memoize": "^1.0.0-alpha.3",
    "underscore": "^1.9.1",
    "validate.js": "^0.13.1",
    "websocket": "^1.0.30"
  },
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/pluralize": "0.0.29",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-navigation": "^3.0.8",
    "@types/realm": "^1.13.0",
    "@types/reflect-metadata": "0.1.0",
    "@types/validate.js": "0.11.0",
    "babel-jest": "24.9.0",
    "babel-preset-react-native": "^4.0.1",
    "concurrently": "^5.0.0",
    "jest": "24.9.0",
    "npm": "^6.12.0",
    "react-test-renderer": "16.11.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "preset": "react-native",
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx)$",
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "artifacts/**/*.js",
      "!artifacts/**/*.spec.js",
      "!artifacts/**/*.index.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
